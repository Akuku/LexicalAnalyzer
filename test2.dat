int gcd(int u, int v) {
   if (v==0) return u;
else return gcd(v, u-u/v*v);
   /* u-u/v*v == u mod v */
}
void sort(int a[], int low, int high) {
   int i; int k;
i = low;
while (i<high-1) {
      int t;
k = minloc(a,i,high);
t = a[k];
a[k] = a[i];
a[i] = t;
i = i + 1;
   }
}
